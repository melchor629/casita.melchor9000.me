{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "link": {
      "dependsOn": ["^lint"]
    },
    "docker": {},
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
